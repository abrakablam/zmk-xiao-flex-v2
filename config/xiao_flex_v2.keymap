#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layout {
            bindings = <
&kp ESC    &kp N1  &kp N2    &kp N3        &kp N4          &kp N5          &kp N6     &kp N7          &kp N8            &kp N9             &kp N0         &kp EQUAL
&kp TAB    &kp Q   &kp W     &kp F         &kp P           &kp B           &kp J      &kp L           &kp U             &kp Y              &kp SEMICOLON  &kp MINUS
&kp LCTRL  &kp A   &kp R     &kp S         &kp T           &kp G           &kp M      &kp N           &kp E             &kp I              &kp O          &kp SINGLE_QUOTE
&kp DEL    &kp Z   &kp X     &kp C         &kp D           &kp V           &kp K      &kp H           &kp COMMA         &kp PERIOD         &kp SLASH      &kp GRAVE
                   &kp LEFT  &kp RIGHT                                                                &kp LEFT_BRACKET  &kp RIGHT_BRACKET
                             &kp ASTERISK  &kp LEFT_ALT    &kp LEFT_WIN    &to 3      &kp UP_ARROW    &kp ENTER
                             &mo 1         &kp LEFT_SHIFT  &kp SPACE       &kp SPACE  &kp DOWN_ARROW  &mo 2
            >;
        };

        symbols {
            bindings = <
&kp              &kp GRAVE       &kp LEFT_PARENTHESIS  &kp RIGHT_PARENTHESIS  &kp SEMICOLON     &kp COMMA       &kp F1     &kp F2                &kp F3            &kp F4             &kp F5                 &kp F6
&kp EXCLAMATION  &kp LEFT_BRACE  &kp SQT               &kp DOUBLE_QUOTES      &kp RIGHT_BRACE   &kp QUESTION    &kp F7     &kp F8                &kp F9            &kp F10            &kp F11                &kp F12
&kp HASH         &kp CARET       &kp EQUAL             &kp UNDERSCORE         &kp DOLLAR        &kp ASTERISK    &kp TILDE  &kp AMPERSAND         &kp LEFT_BRACKET  &kp RIGHT_BRACKET  &kp PLUS               &kp BACKSLASH
&kp AT_SIGN      &kp LESS_THAN   &kp PIPE              &kp MINUS              &kp GREATER_THAN  &kp SLASH       &kp        &kp LEFT_PARENTHESIS  &kp LEFT_BRACE    &kp RIGHT_BRACE    &kp RIGHT_PARENTHESIS  &kp
                                 &kp                   &kp                                                                                       &kp               &kp
                                                       &kp                    &kp               &kp             &kp        &kp                   &kp
                                                       &kp                    &kp               &kp             &kp        &kp                   &kp
            >;
        };

        layer_2 {
            bindings = <
&trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans
&trans  &trans  &trans  &trans  &trans  &trans    &trans  &trans  &trans  &trans  &trans  &trans
                &trans  &trans                                    &trans  &trans
                        &trans  &trans  &trans    &trans  &trans  &trans
                        &trans  &trans  &trans    &trans  &trans  &trans
            >;
        };

        qwerty {
            bindings = <
&trans  &kp N1  &kp N2  &kp N3  &kp N4  &kp N5    &kp N6  &kp N7  &kp N8  &kp N9  &kp N0  &trans
&trans  &kp Q   &kp W   &kp E   &kp R   &kp T     &kp Y   &kp U   &kp I   &kp O   &kp P   &trans
&trans  &kp A   &kp S   &kp D   &kp F   &kp G     &kp H   &kp J   &kp K   &kp L   &trans  &trans
&trans  &kp Z   &kp X   &kp C   &kp V   &kp B     &kp N   &kp M   &trans  &trans  &trans  &trans
                &trans  &trans                                    &trans  &trans
                        &trans  &trans  &trans    &trans  &trans  &trans
                        &to 0   &trans  &trans    &trans  &trans  &trans
            >;
        };
    };
};
